{
	"title": "Foundry Summons",
	"id": "foundry-summons",
	"description": "A Summoning Module.",
	"version": "To Be Replaced",
	"esmodules": ["index.js"],
	"styles": ["style.css"],
	"socket": true,
	"authors": [
		{
			"discord": "Unoblue#0001",
			"email": "Nathan20467@gmail.com",
			"name": "Unoblue"
		}
	],
	"flags": {
		"hotReload": {
			"extensions": [".json"]
		}
	},
	"media": [
		{
			"type": "setup",
			"name": "Setup Screen",
			"url": "modules/foundry-summons/assets/setup.webp"
		}
	],
	"compatibility": {
		"minimum": "12",
		"verified": "12"
	},
	"manifest": "https://github.com/Unoblueboy/foundry-summons/releases/latest/download/module.json",
	"download": "https://github.com/Unoblueboy/foundry-summons/releases/latest/download/module.zip",
	"languages": [
		{
			"lang": "en",
			"name": "English",
			"path": "lang/en.json"
		}
	],
	"relationships": {
		"requires": [
			{
				"id": "portal-lib",
				"type": "module",
				"manifest": "https://github.com/theripper93/portal-lib/releases/latest/download/module.json",
				"compatibility": {
					"minimum": "2.2.11"
				}
			},
			{
				"id": "socketlib",
				"type": "module",
				"manifest": "https://raw.githubusercontent.com/manuelVo/foundryvtt-socketlib/master/module.json",
				"compatibility": {
					"minimum": "1.1.0"
				}
			}
		],
		"optional": [
			{
				"id": "sequencer",
				"type": "module",
				"manifest": "https://github.com/fantasycalendar/FoundryVTT-Sequencer/releases/latest/download/module.json"
			}
		]
	},
	"packs": [
		{
			"name": "macros",
			"label": "Foundry Summons Macros",
			"path": "packs/macros",
			"type": "Macro",
			"banner": "modules/foundry-summons/assets/banner.webp",
			"ownership": {
				"PLAYER": "OWNER"
			}
		}
	],
	"readme": "https://github.com/Unoblueboy/foundry-summons/blob/master/README.md",
	"url": "https://github.com/Unoblueboy/foundry-summons",
	"bugs": "https://github.com/Unoblueboy/foundry-summons/issues",
	"changelog": "https://github.com/Unoblueboy/foundry-summons/releases/latest/"
}
